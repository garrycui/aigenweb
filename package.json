  "name": "aigenweb",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && tsc -p tsconfig.server.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node dist/server/server.js",
